{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "node_modules/**/*.js",
      "*.json",
      "**/*.json"
    ]
  },
  "rules": {
    "max-line-length": false,
    "import-name": false,
    "variable-name": false
  }
}